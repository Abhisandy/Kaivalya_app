<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="online.kaivalya.btkit.kaivalya">
<uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="online.kaivalya.btkit.kaivalya.Splash"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="online.kaivalya.btkit.kaivalya.MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity
            android:name="online.kaivalya.btkit.kaivalya.Events"
            android:label="@string/title_activity_events"
            android:theme="@style/AppTheme"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity
            android:name="online.kaivalya.btkit.kaivalya.Workshops_Saini"
            android:label="Workshops"
            android:theme="@style/AppTheme"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Event"
            android:label="@string/title_activity_event"
            android:theme="@style/AppTheme"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity
            android:name="online.kaivalya.btkit.kaivalya.EventSaini"
            android:label="@string/title_activity_event_saini"
            android:theme="@style/AppTheme"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="online.kaivalya.btkit.kaivalya.AboutKaiv"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="online.kaivalya.btkit.kaivalya.Register_saini"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
    </application>

</manifest>